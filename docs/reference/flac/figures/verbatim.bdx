<?xml version="1.0" encoding="utf-8"?>
<diagram>
  <row>
    <col width=".18">Frame Header</col>
    <col width=".15">Subframe₀</col>
    <col width=".15" id="subframe">Subframe₁</col>
    <col width=".15">Subframe₂</col>
    <col width=".15">Subframe₃</col>
