  </row>
  <line style="dotted">
    <start id="subframe" corner="sw"/>
    <end id="subframe_s" corner="nw"/>
  </line>
  <line style="dotted">
    <start id="subframe" corner="se"/>
    <end id="subframe_e" corner="ne"/>
